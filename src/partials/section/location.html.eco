<!-- <h2 class="section-title"><%= @labels['location'] %></h2> -->


<p itemprop="address" itemscope itemtype="http://schema.org/PostalAddress" class="address">
  <span class="prop" itemprop="streetAddress"><%= @conf.venue %></span><br>
  <span class="prop" itemprop="streetAddress"><%= @conf.venueComplete %></span>,
  <span class="prop" itemprop="addressLocality"><%= @conf.address %></span>
</p>
<div id="map-canvas" class="location-area" data-address="<%= @conf.address %>"></div>