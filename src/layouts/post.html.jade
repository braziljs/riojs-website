h1(class="meetup")= document.talkName

ul(class="speakers")

	li(class="speaker")
		em= "por " + document.speakerName
		!= document.content
		- if (document.youtubeID.length)
			iframe(width="850", height="638", src="http://www.youtube.com/embed/" + document.youtubeID, frameborder="0", allowfullscreen="")